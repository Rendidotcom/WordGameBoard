{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": [
    "frontend/**/*"  // Tambahkan folder frontend agar file register.ts terkompilasi
  ],
  "exclude": [
    "dist"  // Jangan kompilasi folder dist
  ]
}
